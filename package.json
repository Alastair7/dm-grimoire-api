  "description": "DM Grimoire's API REST",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node src/app.ts",
    "nodemon": "nodemon src/app.ts",
    "build": "tsc -p tsconfig.build.json",
    "serve": "node dist/app.js",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alastair7/dm-grimoire-api.git"
  },
  "keywords": [
    "api",
    "rest",
    "roleplaying",
    "d&d",
    "nodeJS",
    "TS",
    "docker"
  ],
  "author": "Senka & Alastair7",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alastair7/dm-grimoire-api/issues"
  },
  "homepage": "https://github.com/Alastair7/dm-grimoire-api#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "reflect-metadata": "^0.2.1"
  }
}
